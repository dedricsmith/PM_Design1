<mjml>
  <mj-head>
    <mj-title>Prevention Network Newsletter</mj-title>
    <mj-font name="Poppins" href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap" />
    <mj-font name="Montserrat" href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500&display=swap" />
    <mj-style>
      * {
        box-sizing: border-box;
      }
    </mj-style>
    <mj-attributes>

      <mj-breakpoint width="450px" />

      <mj-class name="header-line" align="center" border-color="#ffffff" border-width="2px" width="250px" />

      <mj-class name="small-text" font-family="Montserrat, sans-serif" color="#ffffff" font-size="14px" />

      <mj-class name="btn" font-family="Poppins, sans-serif" font-size="15px" font-weight="600" />

      <mj-class name="btn-dark" color="#ffffff" background-color="#290857" />

      <mj-class name="btn-light" color="#401c6b" background-color="#ffffff" />

      <mj-class name="footer-line" align="center" border-color="#ffffff" border-width="2px" width="110px" />
    </mj-attributes>

    <mj-style inline="inline">
      .card {
        background-color: #a378d9;
        border-radius: 5px;
        box-shadow: 5px 6px 4px 0px rgba(0, 0, 0, 25%);
      }

      .title h1,
      h2 {
        margin: 0 0 10px 0;
        color: #ffffff;
        font-size: 22px;
        font-family: Poppins, sans-serif;
      }

      .title h3 {
        color: #ffffff;
        font-size: 22px;
        font-family: Poppins, sans-serif;
      }

      .head-info p {
        line-height: 1.5;
      }

      .content-title h2 {
        color: #ffffff;
        font-family: Poppins, sans-serif;
        font-size: 22px;
        border: 4px solid #ffffff;
        padding: 5px
      }

      .content-title div {
        width: 270px;
      }

      .content-image img {
        border-radius: 30px;
      }

      .content-image div {
        padding-right: 5px;
      }

      .content-text div {
        padding: 10px;
      }

      .content-text h3,
      p {
        color: #ffffff;
      }

      .content-text p {
        line-height: 1.5;
        font-family: Montserrat, sans-serif;
      }

      .programs-title h2 {
        font-size: 22px;
        color: #290857;
      }

      .footer-text p {
        font-family: Montserrat, sans-serif;
        font-size: 12px;
        color: #290857;
        margin: 0px;
        padding: 3px;
      }

      .footer-text a {
        color: #a378d9;
      }

      .footer-links a{
        font-family: Montserrat, sans-serif;
        font-size: 12px;
        color: #290857;
      }

      .footer-links p{
        font-family: Montserrat, sans-serif;
        font-size: 11px;
        color: #290857;
      }

      .footer-image a{
        text-decoration: none;
        color: #290857;
        font-size: 9px;
        font-family: Montserrat, sans-serif;
      }
    </mj-style>

  </mj-head>
